{
  "name": "free-nextjs-admin-dashboard",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "local": "dotenv -e .env.local next dev",
    "dev": "dotenv -e .env.development next dev",
    "build:dev": "dotenv -e .env.development next build",
    "build:prod": "dotenv -e .env.production next build",
    "clean:dev": "rimraf build/dev",
    "mkdir:dev": "powershell -Command \"New-Item -ItemType Directory -Force -Path build\"",
    "copy:dev": "cpx \".next/**/*\" build/dev/.next && cpx \"public/**/*\" build/dev/public && cpx package.json build/dev/ && cpx package-lock.json build/dev/ && cpx ecosystem.config.js build/dev/",
    "package:dev": "npm run clean:dev && npm run mkdir:dev && npm run copy:dev",
    "clean:prod": "rimraf build/prod",
    "mkdir:prod": "powershell -Command \"New-Item -ItemType Directory -Force -Path build\"",
    "copy:prod": "cpx \".next/**/*\" build/prod/.next && cpx \"public/**/*\" build/prod/public && cpx package.json build/prod/ && cpx package-lock.json build/prod/ && cpx ecosystem.config.js build/prod/",
    "package:prod": "npm run clean:prod && npm run mkdir:prod && npm run copy:prod",
    "build-and-package:dev": "npm run build:dev && npm run package:dev",
    "build-and-package:prod": "npm run build:prod && npm run package:prod",
    "start": "next start",
    "lint": "next lint",
    "zip": "git archive -o $npm_package_config_zipPrefix-$npm_package_config_theme-v$npm_package_version.zip -9 HEAD"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "apexcharts": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "flatpickr": "^4.6.13",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "openai": "^6.9.1",
    "react": "^19.2.0",
    "react-apexcharts": "^1.8.0",
    "react-day-picker": "^9.12.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "swiper": "^11.2.10",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@svgr/webpack": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/react-transition-group": "^4.4.12",
    "cpx": "^1.5.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.1.3",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  }
}
